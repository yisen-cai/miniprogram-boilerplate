<!--components/complete-com/complete-com.wxml-->
<view class="complete-container">
  <view class="complete-details">
    <view class="complete-icon">
    </view>
    
    <view class="complete-msg">
      <text>创建{{message}}成功!</text>
    </view>
  </view>

  <view class="complete-actions">
    <view class="button check-button" bindtap="checkResult" hover-class="pressed" hover-stay-time="0" hover-stay-time="0">
      查看
    </view>
    <view class="button back-button" bindtap="backHome" hover-class="pressed" hover-stay-time="0" hover-start-time="0">
      返回主页
    </view>
  </view>
</view>