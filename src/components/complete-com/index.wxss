.complete-msg {
  font-size: 36rpx;
  position: relative;
  left: 20rpx;
  top: 5rpx;
}

.complete-actions {
  height: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

button {
  font-weight: normal;
}


.button {
  width: 400rpx;
  padding: 25rpx;
  text-align: center;
  color: white;
  box-shadow: 0 10rpx 10rpx 0 lightgray;
}

.check-button {
  background: #70e6a7;
}

.back-button {
  background: #82e3d4;
}

.pressed {
  opacity: .8;
}