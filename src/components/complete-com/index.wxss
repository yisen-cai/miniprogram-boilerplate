/* components/complete-com/complete-com.wxss */
.complete-container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 600rpx;
  justify-content: space-around;
}

.complete-details {
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

