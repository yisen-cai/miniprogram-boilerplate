.container {
  padding: 100rpx 0;
}

.create-items {
  display: inline-flex;
  flex-wrap: wrap;
  width: 640rpx;
  align-items: center;
  justify-content: space-around;
}


.item {
  margin-top: 30rpx;
  width: 300rpx;
  height: 256rpx;
  box-shadow: 0 0 20rpx 0 lightgray;
  border-radius: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  background: white;
}

.text {
  font-size: 20px;
}


.item-pressed {
  opacity: .8;
  background: whitesmoke !important;
}


.cover {
  background-size: contain;
  background-repeat: no-repeat;
  width: 100rpx;
  height: 75rpx;
  margin-top: 56rpx;
  margin-left: 40rpx;
  margin-bottom: 30rpx;
}


