.container {
  padding: 28rpx;
  justify-content: flex-start;
}


.user-info-card {
  background: white;
  border-radius: 40rpx;
  height: 320rpx;
  width: 560rpx;
  box-shadow: 0 0 12rpx 0 lightgray;
  display: inline-flex;
  align-items: center;
  padding: 40rpx;
}


.avatar-position {
  display: flex;
  flex-direction: column;
  height: 100%;
  align-items: center;
  justify-content: flex-start;
}


.avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  background-repeat: no-repeat;
  background-size: cover;
}


.position {
  color: white;
  font-size: 20px;
  text-align: center;
  margin-top: 30rpx;
  background: #ff4e4a;
  border: 1px solid red;
  border-radius: 20rpx;
  padding: 0 10rpx;
}


.username-gender {
  width: 270rpx;
  height: 100%;
  padding-left: 30rpx;
}


.username {
  font-size: 20px;
  text-align: left;
  margin-top: 12rpx;
}

.gender {
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
  background-repeat: no-repeat;
  background-size: contain;
}

